{
  "multipart": [
    {
      "apply": {"model": "cropcultivation:block/soil_dry"}
    }, {
      "when": {
        "OR": [{"moisture": "6"}, {"moisture": "7"}]
      }, "apply": {"model": "cropcultivation:block/soil_moist"}
    }, {
      "when": {
        "OR": [{"moisture": "8"}, {"moisture": "9"}]
      }, "apply": {"model": "cropcultivation:block/soil_wet"}
    }, {
      "when": {"moisture": "10"}, "apply": {"model": "cropcultivation:block/soil_waterlogged"}
    }
  ]
}